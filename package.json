{
  "name": "reduxtful",
  "version": "0.3.1",
  "description": "Redux + RESTful",
  "main": "index.js",
  "repository": {
    "type": "git"
  },
  "keywords": [
    "gulp",
    "seed",
    "boilerplate",
    "kickstarter",
    "ES6",
    "6to5"
  ],
  "author": "Rick Chen <xtforgame@gmail.com>",
  "scripts": {
    "lint": "eslint --ext .js ./src",
    "lint-fix": "eslint --fix --ext .js ./src",
    "lint-all": "eslint --ext .js ./src ./test ./internals",
    "lint-all-fix": "eslint --fix --ext .js ./src ./test ./internals",
    "build": "cross-env NODE_ENV=production gulp library:build",
    "start": "cross-env NODE_ENV=development gulp library:watch:dev",
    "test": "eslint --ext .js ./test/library && cross-env NODE_ENV=test mocha --require babel-core/register './test/library/**/*.spec.js'",
    "test-watch": "eslint --ext .js ./test/library && cross-env NODE_ENV=test mocha --watch ./test --watch-extensions js --require babel-core/register './test/library/**/*.spec.js'"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.15.0",
    "az-gulp-env-lite": "git+https://github.com/xtforgame/az-gulp-env-lite#v1.0.0-alpha.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.14.0",
    "chai": "^4.1.2",
    "cross-env": "^3.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.12.0",
    "gulp": "^4.0.0",
    "immutable": "^3.8.1",
    "mocha": "^5.2.0",
    "redux": "^4.0.4",
    "redux-immutable": "^4.0.0",
    "redux-mock-store": "^1.5.1",
    "redux-observable": "^1.1.0",
    "redux-wait-for-action": "^0.0.7",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "rimraf": "^2.5.4",
    "rxjs": "^6.5.2",
    "sinon": "^4.3.0"
  },
  "false": {}
}
