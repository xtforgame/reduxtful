{
  "presets": ["env"],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-object-rest-spread",
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": [
        ["module-resolver", {
          "alias": {
            "~": "./src/test/library",
            "library": "./src/library"
          }
        }]
      ]
    },
    "production": {
      "plugins": [
        ["module-resolver", {
          "alias": {
            "~": "./src/test/library",
            "library": "./src/library"
          }
        }]
      ]
    },
    "test": {
      "plugins": [
        ["module-resolver", {
          "alias": {
            "~": "./src/test/library",
            "library": "./src/library"
          }
        }]
      ]
    }
  }
}
